{
  "name": "exam-genius-backend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve --watch=false",
    "build": "npx prisma generate && nx build",
    "test": "nx test",
    "prisma generate": "npx prisma generate",
    "nx:serve:backend": "nx serve",
    "nx:build:backend": "nx build",
    "nx:build:backend:prod": "npx prisma generate && nx build --prod --skip-nx-cache",
    "nx:serve:backend:prod": "node dist/exam-genius-backend/main.js",
    "create-env-file:backend": "doppler secrets download --no-file --format env > .env -p exam-genius -c loc",
    "create-env-file:backend:prod": "doppler secrets download --no-file --format env > .env.doppler.prod -p exam-genius -c prd"
  },
  "private": true,
  "dependencies": {
    "@fastify/autoload": "~5.7.1",
    "@fastify/cors": "^8.3.0",
    "@fastify/env": "^4.2.0",
    "@fastify/sensible": "~5.2.0",
    "@logtail/node": "^0.5.2",
    "@logtail/winston": "^0.5.2",
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.8",
    "fastify": "~4.19.2",
    "fastify-plugin": "~4.5.0",
    "openai": "^4.29.2",
    "tslib": "^2.3.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nrwl/esbuild": "16.5.0",
    "@nrwl/eslint-plugin-nx": "16.5.0",
    "@nrwl/jest": "16.5.0",
    "@nrwl/js": "16.5.0",
    "@nrwl/linter": "16.5.0",
    "@nrwl/node": "16.5.0",
    "@nrwl/workspace": "16.5.0",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "esbuild": "0.17.17",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "16.5.0",
    "nx-cloud": "16.0.5",
    "prettier": "^2.6.2",
    "prisma": "^5.11.0",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
